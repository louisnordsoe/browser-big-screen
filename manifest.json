{
  "manifest_version": 3,
  "name": "Web TV OS",
  "version": "3.1",
  "description": "TV Dashboard",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "dashboard.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["sidebar.css"],
      "matches": ["<all_urls>"]
    }
  ]
}